	<script type="text/javascript">
		jQuery(document).ready(function(){
			parallax_overide('page-id-1570');
		});

		function parallax_overide(pageid){
			jQuery(window).scroll(function(){
				var getScroll = jQuery(this).scrollTop();
				if(getScroll > 243){
					jQuery('.' + pageid +' .fusion-page-title-bar').css('background-attachment', 'fixed');
					jQuery('.' + pageid +' .fusion-page-title-bar').css('background-size', 'contain');
					jQuery('.' + pageid +' .fusion-page-title-bar').css('background-position-y','21px');
				} else {
					jQuery('.' + pageid +' .fusion-page-title-bar').removeAttr('style');
				}
			});
		}
	</script>
