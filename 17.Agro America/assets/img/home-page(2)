<script type="text/javascript">
jQuery(window).scroll(function() {    
    var scroll = jQuery(window).scrollTop();

    if (scroll >= 50) {
        jQuery("#banner-parallax").addClass("scroll");
       jQuery(".fusion-page-title-bar").addClass("scroll");
      
    } else {
      jQuery("#banner-parallax").removeClass("scroll");
      jQuery(".fusion-page-title-bar").removeClass("scroll");
      jQuery(".home .fusion-page-title-bar").removeClass("scroll");
    }
}); </script>
<!-- end Simple Custom CSS and JS -->
