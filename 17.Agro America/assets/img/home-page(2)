<script type="text/javascript">
jQuery(document).ready(function(){
jQuery('#text-2 a').click(function(e){
 e.preventDefault();
 var thisIndex = jQuery(this).index();
 if (thisIndex == 1){
    window.open('http://www.wbasco.org/index-eng.htm');
 } else if (thisIndex == 2 ){
     window.open('http://www.globalgap.org/uk_en');
 } else if (thisIndex == 3 ){
     window.open('http://www.rainforest-alliance.org/');
 } else if (thisIndex == 4 ){
     window.open('http://www.ok.org/');
 } else if (thisIndex == 5 ){
     window.open('http://www.rspo.org/');
 } else if (thisIndex == 6 ){
     window.open('https://www.fairtrade.net/about-fairtrade/certifying-fairtrade.html');
 } else if (thisIndex == 7 ){
     window.open('https://www.scsglobalservices.com/sustainably-grown-certification');
 }
  
  
});
  });</script>
